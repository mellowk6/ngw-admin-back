<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nhbank.ngw.infra.user.mapper.DeptMapper">

    <resultMap id="DeptMap" type="com.nhbank.ngw.domain.user.model.Dept">
        <constructor>
            <arg column="dept_code" javaType="java.lang.String" name="code"/>
            <arg column="dept_name" javaType="java.lang.String" name="name"/>
        </constructor>
    </resultMap>

    <select id="selectAll" resultMap="DeptMap">
        select dept_code, dept_name
        from dept
        order by dept_code
    </select>
</mapper>